{
  "name": "starter",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "watch": "nodemon --watch 'src/backend/**/*.ts' --ignore 'src/backend/**/*.spec.ts' --exec 'ts-node' src/backend/server.ts",
    "build": "./node_modules/typescript/bin/tsc -p .",
    "migrate": "./node_modules/ts-node/dist/bin.js ./src/backend/app/config/migrate.ts",
    "spec": "NODE_ENV=test mocha --require ts-node/register --recursive 'src/backend/spec/**/*_spec.ts' --exit",
    "seed": "ts-node ./src/backend/app/config/seed.ts",
    "lint": "eslint . --ext .ts"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~4.1.1",
    "dotenv": "^8.2.0",
    "ejs": "~3.1.3",
    "express": "~4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mocha": "^8.0.1",
    "morgan": "~1.10.0",
    "multer": "^1.4.2",
    "node-pg-migrate": "^5.3.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.3.0",
    "http-errors": "^1.8.0",
    "sequelize": "^6.3.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/chai": "^4.2.12",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.7",
    "@types/http-errors": "^1.8.0",
    "@types/mocha": "^8.0.0",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.0.26",
    "@types/passport": "^1.0.4",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-local": "^1.0.33",
    "@types/validator": "^13.1.0",
    "@typescript-eslint/eslint-plugin": "^3.7.1",
    "@typescript-eslint/parser": "^3.7.1",
    "eslint": "^7.5.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "nodemon": "^2.0.4",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  }
}
